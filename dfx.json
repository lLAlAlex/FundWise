{
  "canisters": {
    "user_backend": {
      "type": "motoko",
      "main": "backend/user_backend/main.mo",
      "declarations": {
        "node_compatibility": true
      }
    },
    "utils_backend": {
      "type": "motoko",
      "main": "backend/utils_backend/main.mo"
    },
    "company_backend": {
      "type": "motoko",
      "main": "backend/company_backend/main.mo",
      "dependencies": [
        "utils_backend"
      ],
      "declarations": {
        "node_compatibility": true
      }
    },
    "frontend": {
      "dependencies": [
        "user_backend",
        "company_backend"
      ],
      "type": "assets",
      "source": [
        "dist/"
      ]
    },
    "internet_identity": {
      "type": "pull",
      "id": "rdmx6-jaaaa-aaaaa-aaadq-cai"
    }
  },
  "defaults": {
    "build": {
      "packtool": "npm run --silent sources"
    }
  },
  "output_env_file": ".env",
  "version": 2
}